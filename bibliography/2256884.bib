@article{helmig_inventar_1991,
	title = {Inventar der Basler Stadtbefestigungen. Planvorlage und Katalog. 1. Die landseitige Äussere Grossbasler Stadtmauer},
	volume = {1989},
	issn = {2673-8678, 1424-4535},
	url = {https://eterna.unibas.ch/index.php/bodenforschungjb/article/view/498},
	doi = {10.12685/jbab.1989.69-153},
	shorttitle = {Inventar der Basler Stadtbefestigungen - Planvorlage und Katalog},
	abstract = {Beim vorliegenden Inventar der Basler Stadtbefestigungen handelt es sich um den ersten Teil, der die landseitige Äussere Grossbasler Stadtmauer thematisiert. Der zweite Teil behandelt die rheinseitigen Grossbasler Stadtbefestigungen. 
Ziel war es, den genauen Verlauf der Stadtmauer zu illustrieren sowie den Kenntnisstand zu den verschiedenen archäologischen Aufschlüssen zusammenzustellen. Es war uns ein Anliegen, die Ergebnisse der Untersuchungen nicht nur in einem Plan festzuhalten und ein Inventar der mittelalterlichen Stadtmauern herzustellen, vielmehr wollten wir die Stadtbefestigungen als kulturgeschichtliches Denkmal begreifen und Informationen über die ganze Zeit ihres rund 500jährigen Bestehens sammeln.},
